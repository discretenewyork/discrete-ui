{
  "name": "discrete-ui",
  "version": "3.0.0",
  "description": "Discrete New York UI Component Kit",
  "author": "Elias Martinez Cohen <elias@discretenewyork.com>",
  "license": "MIT",
  "main": "dist/discrete-ui.js",
  "scripts": {
    "build": "npm run clean && webpack --config config/prod.js",
    "build:demo": "webpack --config config/demo.js",
    "clean": "rm -rf dist/",
    "start": "npm run demo",
    "demo": "webpack-dev-server --hot --config config/demo.js",
    "lint": "eslint src/"
  },
  "repository": "git+https://github.com/discretenewyork/discrete-ui.git",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-eliath": "^2.1.0",
    "html-webpack-plugin": "^3.1.0",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-input-mask": "^1.2.2",
    "shortid": "^2.2.8",
    "styled-components": "^3.2.3",
    "svg-sprite-loader": "^3.7.3",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "peerDependencies": {
    "polished": "1.x",
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-input-mask": "1.x",
    "shortid": "2.x",
    "styled-components": "3.x"
  },
  "eslintConfig": {
    "extends": "eliath",
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "import"
    ]
  }
}
